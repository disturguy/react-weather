{
  "priority": 2,
  "request": {
    "method": "GET",
    "urlPath": "/weather",
    "queryParameters": {
      "lat": {
        "matches": "[+-a-zA-Z0-9.]+\\.[a-zA-Z0-9.]+"
      },
      "lon": {
        "matches": "[+-a-zA-Z0-9.]+\\.[a-zA-Z0-9.]+"
      },
      "appid": {
        "matches": "[a-zA-Z0-9.]+"
      }
    }
  },
  "response": {
    "status": 200,
    "bodyFileName": "current-weather-by-coordinates.json",
    "headers": {
      "Content-Type": "application/json"
    }
  }
}